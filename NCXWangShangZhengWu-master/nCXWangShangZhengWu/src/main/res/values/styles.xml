<resources xmlns:android="http://schemas.android.com/apk/res/android">

    <!--
        Base application theme, dependent on API level. This theme is replaced
        by AppBaseTheme from res/values-vXX/styles.xml on newer devices.

    -->
    <style name="AppBaseTheme" parent="android:Theme.Light">
        <!--
            Theme customizations available in newer API levels can go in
            res/values-vXX/styles.xml, while customizations related to
            backward-compatibility can go here.

        -->
    </style>

    <!-- Application theme. -->
    <style name="AppTheme" parent="AppBaseTheme">
        <!-- All customizations that are NOT specific to a particular API-level can go here. -->
    </style>
       <style name="MMTabButton">
		<item name="android:textSize">0.0dip</item>
		<item name="android:gravity">center_horizontal</item>
		<item name="android:background">@null</item>
		<item name="android:layout_width">fill_parent</item>
		<item name="android:layout_height">wrap_content</item>
		<item name="android:button">@null</item>
		<item name="android:textColor">@color/white</item>
		<item name="android:layout_weight">1.0</item>
		<item name="android:paddingBottom">0.0dip</item>
		<item name="android:paddingTop">0.0dip</item>
	</style>
    <string-array name="zhengjian">  
        <item>身份证</item>  
        <item>军官证</item>          
        <item>工作证</item>  
        <item>学生证</item>          
    </string-array>
    <string-array name="type">  
        <item>咨询</item>  
        <item>投诉</item>          
        <item>建议</item>  
        <item>意见</item>
        <item>其它</item>  
        <item>求助</item>          
    </string-array>    
       
     
           <!-- Dialog -->
    <style name="dialog_loading" parent="@android:style/Theme.Dialog">
        <item name="android:windowFrame">@null</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowBackground">@color/transparent</item>
        <item name="android:background">@color/transparent</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:backgroundDimEnabled">false</item>
    </style>
<style name="list_item_middle">
		<item name="android:clickable">false</item>
		<item name="android:paddingTop">5dip</item>
		<item name="android:paddingBottom">5dip</item>
		<item name="android:paddingLeft">10dip</item>
    	<item name="android:paddingRight">10dip</item>
    	<item name="android:gravity">center_vertical</item>
    	<item name="android:background">@drawable/background_view_rounded_middle</item>
	</style>
	<!-- TEXT -->
	<style name="content_page_large_text">
		<item name="android:duplicateParentState">true</item>
        <item name="android:textSize">16sp</item>
        <item name="android:typeface">sans</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">@color/text_color_selector</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:ellipsize">end</item>
        <item name="android:singleLine">true</item>
    </style>
    <style name="list_item_chevron">
        <item name="android:paddingLeft">4.0dip</item>
        <item name="android:paddingTop">2.0dip</item>
        <item name="android:paddingRight">4.0dip</item>
        <item name="android:paddingBottom">2.0dip</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:src">@drawable/chevron</item>
        <item name="android:duplicateParentState">true</item>
    </style>	
    <style name="list_item_point">
        <item name="android:paddingLeft">4.0dip</item>
        <item name="android:paddingTop">2.0dip</item>
        <item name="android:paddingRight">4.0dip</item>
        <item name="android:paddingBottom">2.0dip</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:src">@drawable/bg_point</item>
        <item name="android:duplicateParentState">true</item>
    </style>	
        <style name="tv_default">
        <item name="android:textSize">16sp</item>
        <item name="android:textColor">@color/gray_deep</item>
    </style>
     <!-- GZLM_Title theme. -->
     
    <style name="text_on_button">
        <item name="android:textSize">15.0sp</item>
        <item name="android:textColor">#ffffffff</item>
        <item name="android:shadowRadius">0.0</item>
    </style>

    <style name="common_button" parent="@style/text_on_button">
        <item name="android:background">@drawable/btn_common</item>
        <item name="android:paddingLeft">6.0dip</item>
        <item name="android:paddingRight">6.0dip</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:minWidth">50.0dip</item>
        <item name="android:minHeight">30.0dip</item>
    </style>
         <style name="title_bar_button" parent="@style/common_button">
