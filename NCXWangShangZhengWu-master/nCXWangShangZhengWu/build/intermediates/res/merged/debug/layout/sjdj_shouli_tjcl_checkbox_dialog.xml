<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/ll_detail_jbxx_shff"
    android:layout_width="fill_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical"
    android:background="@drawable/linearlayout" >

    <ListView
        android:layout_weight="1"
        android:id="@+id/lv_sjdj_shouli_tjcl_checkbox"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_margin="@dimen/height_1_100"
        android:cacheColorHint="@android:color/transparent"
        android:divider="#00000000"
        android:dividerHeight="@dimen/height_1_100"
        android:listSelector="@android:color/transparent"
        android:scrollbars="none" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:layout_marginBottom="@dimen/height_1_100" >

        <Button
            android:id="@+id/btnEnsure"
            android:layout_width="@dimen/width_20_100"
            android:layout_height="25dp"
            android:layout_marginLeft="@dimen/width_10_100"
            android:layout_marginRight="@dimen/width_10_100"
            android:layout_weight="1"
            android:background="@drawable/btn_login"
            android:text="@string/ensure" />

        <Button
            android:id="@+id/btn_cancel"
            android:layout_width="@dimen/width_20_100"
            android:layout_height="25dp"
            android:layout_marginLeft="@dimen/width_10_100"
            android:layout_marginRight="@dimen/width_10_100"
            android:layout_weight="1"
            android:background="@drawable/btn_login"
            android:text="@string/cancel" />
    </LinearLayout>

</LinearLayout>